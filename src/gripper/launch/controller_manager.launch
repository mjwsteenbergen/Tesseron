<!-- -*- mode: XML -*- -->

<launch>
    <node name="dynamixel_manager" pkg="dynamixel_controllers" type="controller_manager.py" required="true" output="screen">
        <param name="serial_ports/pan_tilt_port/port_name" value="/dev/ttyUSB0"></param>
        <rosparam>
            namespace: dxl_manager
            serial_ports:
                pan_tilt_port:
                    baud_rate: 57142
                    min_motor_id: 1
                    max_motor_id: 25
                    update_rate: 20
        </rosparam>
    </node>
</launch>
